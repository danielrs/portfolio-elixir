{
    "AWSEBDocerrunVersion": "1",
    "volumes": [
        {
            "name": "portfolio",
            "host": {
                "sourcePath": "/app"
            }
        }
    ],
    "containerDefinitions": [
        {
            "name": "portfolio",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 4000
                }
            ]
        }
    ]
}
